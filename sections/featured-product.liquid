{%- liquid
  assign featured_product = section.settings.featured_product
  assign current_variant = featured_product.selected_or_first_available_variant
  assign featured_media = current_variant.featured_media | default: featured_product.featured_media
  assign first_3d_model = featured_product.media | where: "media_type", "model" | first
  assign gallery_size = section.settings.gallery_size

  if gallery_size == "small"
    assign desktop_image_container_percentage = 46
  elsif gallery_size == "medium"
    assign desktop_image_container_percentage = 56
  elsif gallery_size == "large"
    assign desktop_image_container_percentage = 64
  elsif gallery_size == "xlarge"
    assign desktop_image_container_percentage = 72
  endif

  assign desktop_image_percentage_width = desktop_image_container_percentage
-%}

{% comment %}account for gutter between columns and page outer space{% endcomment %}
{% capture desktop_image_sizes_attribute %}{{ desktop_image_percentage_width }}vw{% endcapture %}

{%- assign featured_media_size = featured_media.width | append: 'x' -%}
{%- if featured_product.featured_media -%}
  {% capture product_featured_image_url -%}
    {{- featured_product.featured_media | image_url: featured_media_size | prepend: "https:" | json -}}
  {%- endcapture %}
{% endif %}
<div
  class="
    featured-product
    product
    section
    section--divider-{{ section.settings.divider_style }}
    {{ section.settings.section_padding }}
    {% if settings.enable_section_animations and section.settings.enable_animation %}
      animation
      animation--product
    {% endif %}
  "
  data-section-id="{{ section.id }}"
  data-section-type="featured-product"
  data-current-product-id="{{ current_variant.id }}"
  data-product-has-only-default-variant="{{ featured_product.has_only_default_variant }}"
  data-gallery-size="{{ gallery_size }}"
  data-is-full-product="true"
  data-is-featured-product="true"
>
  <div class="product__top product__primary">
    <div class="product__primary-left">
      <div class="product__media-container below-mobile carousel swiper{% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}">
        <div class="product__media carousel__wrapper swiper-wrapper">
          {%- for media in featured_product.media -%}
            {%- liquid
              assign prioritize_loading = false

              if forloop.first
                assign prioritize_loading = true
              endif
            -%}
            <div
              class="product__media-item carousel_slide swiper-slide"
              data-media-item-id="{{ media.id }}"
              data-media-type="{{ media.media_type }}"
              data-product-media-wrapper
              data-aspect-ratio="{{ section.settings.image_aspect_ratio }}"
            >
              {%
                render 'media' with
                fallback_alt: featured_product.title,
                media: media,
                featured_media: featured_media,
                enable_product_lightbox: section.settings.enable_product_lightbox,
                loop_video: section.settings.enable_video_looping,
                aspect_ratio: section.settings.image_aspect_ratio,
                sizes: "90vw",
                prioritize_loading: prioritize_loading,
                within_carousel: true
              %}
            </div>
          {%- else -%}
            {%-
              render 'placeholder-image' with
              wrapper_class: 'product__media-item placeholder-image',
              aspect_ratio: section.settings.image_aspect_ratio,
              natural_fallback_ratio: 'square'
            -%}
          {%- endfor -%}
        </div>
        <div class="swiper-pagination"></div>
      </div>
      <div
        class="product__media-container above-mobile{% if section.settings.enable_product_lightbox %} lightbox-media-container{% endif %}"
        data-gallery-style="list"
      >
        <div class="product__media" data-media-count="{{ product.media.size }}">
          {%- for media in featured_product.media -%}
            {%- liquid
              assign animate_media = false

              if media.id == featured_media.id
                assign animate_media = true
              endif
            -%}
            <div
              class="
                product__media-item
                {% unless media.id == featured_media.id %}hidden{% endunless %}
                {% if animate_media %}animation--product-media{% endif %}
              "
              data-media-item-id="{{ media.id }}"
              data-product-media-wrapper
              {% if media.media_type == "image" or media.media_type == "model" %}
                data-aspect-ratio="{{ section.settings.image_aspect_ratio }}"
              {% endif %}
            >
              {%
                render 'media' with
                media: media,
                fallback_alt: featured_product.title,
                featured_media: featured_media,
                enable_product_lightbox: section.settings.enable_product_lightbox,
                loop_video: section.settings.enable_video_looping,
                aspect_ratio: section.settings.image_aspect_ratio,
                sizes: desktop_image_sizes_attribute,
                animate: animate_media
              %}
            </div>
          {%- else -%}
            {%-
              render 'placeholder-image' with
              wrapper_class: 'product__media-item placeholder-image',
              aspect_ratio: section.settings.image_aspect_ratio,
              natural_fallback_ratio: 'square'
            -%}
          {%- endfor -%}
        </div>
      </div>

      {%- if first_3d_model -%}
        <button
          aria-label="{{ 'products.product.view_in_space_label' | t }}"
          class="product__view-in-space btn btn--primary btn--full"
          data-in-your-space
          data-shopify-xr
          data-shopify-model3d-id="{{ first_3d_model.id }}"
          data-shopify-title="{{ featured_product.title | escape }}"
          data-shopify-xr-hidden
        >
          <span class="fs-body-75">
            {% render 'icon' with icon: '3d' %}
            <span class="product__view-in-space-text">{{ 'products.product.view_in_space' | t }}</span>
          </span>
        </button>
      {%- endif -%}

      {% capture left_side_blocks %}
        {%-
          render 'product-blocks' with
          blocks: section.blocks,
          prod: featured_product,
          is_quick: false,
          is_featured: true,
          column: 'left'
        -%}
      {% endcapture %}

      {% if left_side_blocks.size > 10 %}
        <div class="left-side-blocks for-desktop">
          {{ left_side_blocks }}
        </div>
      {% endif %}
    </div>

    <div class="product__details product__primary-right">
      <div class="product__meta">
        {%-
          render 'product-blocks' with
          blocks: section.blocks,
          prod: featured_product,
          is_quick: false,
          is_featured: true,
          column: 'right',
          current_variant: current_variant,
          featured_media: featured_media
        -%}
        {%- comment -%}
          Live region for announcing updated price and availability to screen readers
        {%- endcomment -%}
        <p
          class="visually-hidden"
          data-product-status
          aria-live="polite"
          role="status"
        ></p>

        {%- comment -%}
          Live region for announcing that the product form has been submitted and the
          product is in the process being added to the cart
        {%- endcomment -%}
        <p
          class="visually-hidden"
          data-loader-status
          aria-live="assertive"
          role="alert"
          aria-hidden="true"
        >
          {{ 'products.product.loader_label' | t }}
        </p>
      </div>
    </div>
  </div>

  <div class="product__bottom">
    {% if left_side_blocks.size > 10 %}
      <div class="left-side-blocks for-mobile">
        {{ left_side_blocks }}
      </div>
    {% endif %}
  </div>
</div>

{%- assign models = featured_product.media | where: 'media_type', 'model' | json -%}

<script>
  window.ShopifyXR=window.ShopifyXR||function(){(ShopifyXR.q=ShopifyXR.q||[]).push(arguments)}
  ShopifyXR('addModels', {{ models }});
</script>
<script src="//cdn.shopify.com/shopifycloud/shopify-xr-js/assets/v1.0/shopify-xr.en.js" defer="defer"></script>

{%- unless product == empty -%}
  <script type="application/json" id="ProductJson-{{ section.id }}">
    {{ product | json }}
  </script>
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ featured_product.media | where: 'media_type', 'model' | json }}
  </script>
{%- endunless -%}

